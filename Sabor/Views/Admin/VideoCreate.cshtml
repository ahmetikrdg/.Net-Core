@model VideoModel

<div class="container">
    <h2>Vertical (basic) form</h2>
    <form asp-controller="Admin" asp-action="VideoCreate" method="post" enctype="multipart/form-data">

        <div class="form-group">
            <label for="email"></label>
            <input type="text" class="form-control" placeholder="" asp-for="Title">
        </div>

        <div class="form-group">
            <label for="email"></label>
            <input type="text" class="form-control" placeholder="Url gir" asp-for="Url">
        </div>


        <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>